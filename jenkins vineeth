pipeline {
    agent any
    parameters {
        choice(name: 'VERSION', choices: ['DEV', 'SIT', 'PERF'])
        booleanParam(name: 'Run', defaultValue: true)
    }
    stages {
        stage('Clone Application Code') {
            steps {
                git 'https://github.com/ramdevops03/Development-Team-Repo.git'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn package -f pom.xml'
            }
        }
     }
 }
